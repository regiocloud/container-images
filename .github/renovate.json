{
    "extends": [
        "github>osism/renovate-config",
        "github>osism/renovate-config:docker",
        "github>osism/renovate-config:python"
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "^\\.github\\/workflows\\/build-.*\\.yml"
            ],
            "matchStrings": [
                "- (?<currentValue>.*?)  # renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\n"
            ]
        }
    ]
}
